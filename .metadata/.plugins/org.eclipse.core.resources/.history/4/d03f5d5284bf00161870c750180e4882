package org.o7planning.tutorial.hellomaven;

import org.apache.commons.lang3.StringUtils;

public class CheckNumberic {
	public static void main(String args[]){
		String text1 = "aaaa";
		String text2 = "bbb";
		
		boolean result1 = StringUtils.isNumeric(text1);
		boolean result2 = StringUtils.isNumeric(text2);
		System.out.println(text1 + "Is a number:"+result1);
		System.out.println(text2 +"Is a number"+ result2);
		
	}

}
